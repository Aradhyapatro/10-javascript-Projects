<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-F3w7mX95PdgyTmZZMECAngseQB83DfGTowi0iMjiWaeVhAn4FJkqJByhZMI3AhiU" crossorigin="anonymous">

    <!-- CSS link -->
    <link rel="stylesheet" href="/css/Pro-2/style.css">

    <!-- Title -->
    <title>Quiz App</title>
</head>

<body>


    <div class="card mb-3 Quiz-container">
        <div class="card-body">
            <h4 class="card-title text-center" id="Question">Card title</h4>
            <ul>
                <li class="option1 op">
                    <input type="radio" name="answer" class="answer" id="a">
                    <label for="a" id="aa">Option</label>
                </li>
                <li class="option2 op">
                    <input type="radio" name="answer" class="answer" id="b">
                    <label for="b" id="bb">Option</label>
                </li>
                <li class="option3 op">
                    <input type="radio" name="answer" class="answer" id="c">
                    <label for="c" id="cc">Option</label>
                </li>
                <li class="option4 op">
                    <input type="radio" name="answer" class="answer" id="d">
                    <label for="d" id="dd">Option</label>
                </li>
            </ul>
            <button type="submit" id="Submit">Submit</button>
        </div>
    </div>



    <!-- My JS -->
    <script src="/js/Pro-2/index.js"></script>

    <!-- BootStrap Javascript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-/bQdsTh/da6pkI1MST/rWKFNjaCP5gBSY4sEBT38Q/9RBh9AH40zEOg7Hlq2THRZ" crossorigin="anonymous"></script>
</body>

</html>